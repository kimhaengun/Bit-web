<script type="text/javascript">
var xhr;
window.onload = function(){
   xhr = new XMLHttpRequest();
   xhr.onreadystateChange = function(){
	   console.log('?????');
      if(xhr.readyState == 4 && xhr.status == 200){
         console.log(xhr.responseXML);
      }      
   }
   xhr.open('get','ex11.xml');
   xhr.send();
}
</script>
